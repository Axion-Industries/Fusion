{
  "version": 2,
  "redirects": [
    {
      "source": "/en/index.html",
      "destination": "/en",
      "permanent": true
    },

    {
      "source": "/jp/index.html",
      "destination": "/jp",
      "permanent": true
    },
    {
      "source": "/:section((?!company|en|jp)[^/]+)/index.html",
      "destination": "/:section",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/company",
      "destination": "/company/index.html"
    },
    {
      "source": "/company/index.html",
      "destination": "/company"
    },
    {
      "source": "/jp",
      "destination": "/jp/index.html"
    },
    {
      "source": "/:section((?!company|en|jp)[^/]+)",
      "destination": "/:section/index.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}